{
    "version": 2,
    "builds": [
    {
        "src": "index.js",
        "use": "@vercel/node"
    }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
            },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api/$1" }]
}